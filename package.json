    "pretty": "prettier --write \"./**/*.{js,jsx,css,ts,tsx,json}\"",
    "dev": "vinxi dev --host --port 3001",
    "build": "vinxi build --no-minify",
    "startDev": "dotenv -e .env -- vinxi start --port 3001",
